if("<%= @whipe_users %>"){
  $('.old-users').empty();
  $('.add_pagination').append("<%= j render 'users/paggination', users: @users %>");
  
  history.replaceState(null, "", "?query=<%=params[:query]%>")
}

$('.add_users').append("<%= j render 'users/index', users: @users %>");

<% if (@users.next_page) %>
    $('.pagination').replaceWith('<%= j paginate(@users) %>');
<% else %>
    $('.pagination').remove();
<% end %>


endless_users();