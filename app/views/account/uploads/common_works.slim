/= debug current_account_user
- session[:return_url]         = request.url
- session[:go_to_after_update] = request.url
- blog      = Blog.cached_find('Upload Common Works')
#wrap
  div class="#{'profile' if account_owner?}"
    .full-width-top
      == render 'admin/shared/navbar', page: 'account' 
  
  .container
    == render 'shared/breadcrumbs', breadcrumbs: { links: { @account.title.to_sym => account_path(@account), \
                                                            manage: account_account_path(@account), \
                                                            upload: account_account_uploads_path(@account) \
                                                           }, \
                                                    current:  'common works'}
    ==render 'admin/shared/messages'

    /- insert = 'accounts/shared/leave_account'
    
    == render 'shared/page_title', locals: {blog: blog, blog_post: 'Upload Common Works'}
    

                                          
    .row
      .col-sm-6
        == render 'shared/badge', locals: {  \
                                            blog: blog, \
                                            post: BlogPost.cached_find( "IMPORT CSV FILE" , blog ), \
                                            link: account_account_common_works_path(@account), \
                                            css_class: 'btn btn-primary', \
                                            icon: 'fa fa-music', \
                                            id: 'common-works' \
                                          }
      
      
      
        
      .col-sm-6
        == render 'shared/badge', locals: {  \
                                            blog: blog, \
                                            post: BlogPost.cached_find( "IMPORT FROM PRO" , blog ), \
                                            link: account_account_uploads_common_works_path(@account), \
                                            css_class: 'btn btn-success', \
                                            icon: 'fa fa-database', \
                                            id: 'add-common-work' \
                                          }
      


            

      .col-sm-6
        == render 'shared/badge', locals: {  \
                                            blog: blog, \
                                            post: BlogPost.cached_find( "UPLOAD AUDIO FILE" , blog ), \
                                            link: account_account_recordings_path(@account), \
                                            css_class: 'btn btn-info', \
                                            icon: 'fa fa-cloud-upload', \
                                            id: 'recordings' \
                                          }
        
      
                                    
      
          
      
    
   

        
  
    
-cache (['footer', Admin.cached_find(1).version]) do      
  =render 'shared/footer'
